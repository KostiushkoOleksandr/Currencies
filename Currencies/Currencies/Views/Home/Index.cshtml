
@model IEnumerable<Currencies.Models.EmployeeModel>

    @{ 
        ViewData["Title"] = "Index";
    }

    <h1>Index</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CurrencyCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CurrencyCodeL)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Amount)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <th>
                    @Html.DisplayFor(modelItem => item.CurrencyCode)
                </th>
                <th>
                    @Html.DisplayFor(modelItem => item.CurrencyCodeL)
                </th>
                <th>
                    @Html.DisplayFor(modelItem => item.Amount)
                </th>
            </tr>

        }
    </tbody>
</table>